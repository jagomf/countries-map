{
  "name": "countries-map-dev",
  "version": "3.13.0",
  "description": "Builder and demo runner for countries-map",
  "license": "MIT",
  "author": "Jago MF <jago84@gmail.com>",
  "repository": "github:jagomf/countries-map",
  "homepage": "https://github.com/jagomf/countries-map",
  "scripts": {
    "demo": "npm run build && ng serve",
    "build": "ng build countries-map && npm run dist:copy",
    "build:prod": "ng build countries-map --configuration=production && npm run dist:copy",
    "start": "npm run demo",
    "dist": "npm run build:prod",
    "dist:publish": "npm run dist && cd dist/countries-map && npm publish",
    "dist:copy": "cp ./{README.md,LICENSE} dist/countries-map/",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^17.3.6",
    "@angular/compiler": "^17.3.6",
    "@angular/core": "^17.3.6",
    "@angular/platform-browser": "^17.3.6",
    "@angular/platform-browser-dynamic": "^17.3.6",
    "@jagomf/countrieslist": "^3.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular-eslint/builder": "17.4.1",
    "@angular-eslint/eslint-plugin": "17.4.1",
    "@angular-eslint/eslint-plugin-template": "17.4.1",
    "@angular-eslint/schematics": "17.4.1",
    "@angular-eslint/template-parser": "17.4.1",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.6",
    "@types/google.visualization": "0.0.57",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "^8.57.0",
    "ng-packagr": "^17.3.0",
    "ts-node": "~8.3.0",
    "typescript": "~5.4.5"
  }
}